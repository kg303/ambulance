<!DOCTYPE html>
<html lang="{{ app.request.locale }}">

<head>
    {% apply spaceless %}
        <meta charset="utf-8">
        <meta content="width=device-width, initial-scale=1.0" name="viewport">

        <meta content="" name="description">
        <meta content="" name="keywords">

        <!-- Favicons -->
        {% do pimcore_head_link().appendStylesheet(asset("static/assets/img/favicon.png")) %}
        {% do pimcore_head_link().appendStylesheet(asset("static/assets/img/apple-touch-icon.png")) %}

        <!-- Google Fonts -->
        {% do pimcore_head_link().appendStylesheet("https://fonts.gstatic.com") %}
        {% do pimcore_head_link().appendStylesheet("https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Nunito:300,300i,400,400i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i") %}

        <!-- Vendor CSS Files -->
        {% do pimcore_head_link().appendStylesheet(asset("static/assets/vendor/bootstrap/css/bootstrap.min.css")) %}
        {% do pimcore_head_link().appendStylesheet(asset("static/assets/vendor/bootstrap-icons/bootstrap-icons.css")) %}
        {% do pimcore_head_link().appendStylesheet(asset("static/assets/vendor/boxicons/css/boxicons.min.css")) %}
        {% do pimcore_head_link().appendStylesheet(asset("static/assets/vendor/quill/quill.snow.css")) %}
        {% do pimcore_head_link().appendStylesheet(asset("static/assets/vendor/quill/quill.bubble.css")) %}
        {% do pimcore_head_link().appendStylesheet(asset("static/assets/vendor/remixicon/remixicon.css")) %}
        {% do pimcore_head_link().appendStylesheet(asset("static/assets/vendor/simple-datatables/style.css")) %}

        <!-- Template Main CSS File -->
        {% do pimcore_head_link().appendStylesheet(asset("static/assets/css/style.css")) %}

        {% if not document is defined or not document %}#}
            {% set document = pimcore_document(1) %}
        {% endif %}

        {% if document is instanceof('\\Pimcore\\Model\\Document\\Link') %}
            @var document \Pimcore\Model\Document\Link
            {% set document = document.getElement() %}
        {% endif %}


        {% if document is instanceof('\\Pimcore\\Model\\Document\\Page') %}
            {% if document.getTitle() is not empty %}
                {% do pimcore_head_title().set(document.getTitle()) %}
            {% endif %}

            {% if document.getDescription() is not empty %}
                {% do pimcore_head_meta().setDescription(document.getDescription()) %}
            {% endif %}

            {% do pimcore_head_title().append('Ambulance') %}
            {% do pimcore_head_title().setSeparator(' : ') %}
        {% endif %}

        {% set isPortal = ((isPortal is defined) and isPortal) %}

    {% endapply %}

    {% block layout_head_meta deferred %}
    {{ pimcore_head_title() }}
    {{ pimcore_head_meta() }}

    {% if pimcore_placeholder('canonical').count() %}
        <link rel="canonical" href="{{ pimcore_placeholder('canonical') }}"/>
    {% endif %}

    {% endblock %}

    {% block head_stylesheets deferred %}
    {{ pimcore_head_link() }}
    {% endblock %}

</head>

<body>


{% block content %}
{% endblock %}




{% if not editmode %}
    {{ pimcore_inc(document.getProperty('footer')) }}
{% endif %}

<a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

{% block headscripts %}
    {{ pimcore_head_script() }}
{% endblock %}


{% block inlinescripts deferred %}
{{ pimcore_inline_script() }}
{% endblock %}

<!-- Vendor JS Files -->
<script src="static/assets/vendor/apexcharts/apexcharts.min.js"></script>
<script src="static/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
<script src="static/assets/vendor/chart.js/chart.umd.js"></script>
<script src="static/assets/vendor/echarts/echarts.min.js"></script>
<script src="static/assets/vendor/quill/quill.min.js"></script>
<script src="static/assets/vendor/simple-datatables/simple-datatables.js"></script>
<script src="static/assets/vendor/tinymce/tinymce.min.js"></script>
<script src="static/assets/vendor/php-email-form/validate.js"></script>

<!-- Template Main JS File -->
<script src="static/assets/js/main.js"></script>

</body>

</html>